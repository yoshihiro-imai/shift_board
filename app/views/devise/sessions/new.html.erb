<div class="contents row">
  <div class="container">
    <div class = login>ログインページ</div>
  <div class='login-flash-message'>
      <%= flash[:notice] %>
      <%= flash[:alert] %>
    </div>
    <%= form_with model: @user, url: user_session_path, id: 'new_user', class: 'new_user', local: true do |f| %>      <div class="field">
        <%= f.label "email" %><br />
        <%= f.email_field :email, autofocus: true,class: :form__text %>
      </div>

      <div class="field">
        <%= f.label "password" %><br />
        <%= f.password_field :password, autocomplete: "off",class: :form__text %>
      </div>



      <div class="actions">
        <%= f.submit "ログイン",class: :btn_square %>
      </div>
    <% end %>
  </div>
</div>