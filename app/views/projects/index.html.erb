
<table>
  <thead>
    
  <%= "シフト一覧"%>
    
  </thead>

  <tbody>
    <% @projects.each do |project| %>
      <tr>
        <td><%=project.nickname %></td>  
           <% if user_signed_in? && current_user.id == project.user.id %>
        <td><%= link_to 'Show', project %></td>
        <td><%= link_to 'Edit', edit_project_path(project) %></td>
        <td><%= link_to 'Destroy', project, method: :delete, data: { confirm: 'Are you sure?' } %></td>
        <%end%>
      </tr>
    <% end %>
  </tbody>
</table>
<table>

  <tr >
      <th>日にち</th>

      <th>名前</th>
    <th>IN：OUT</th>

  </tr>
  <% @protest.each do |test|%>
  <tr>
       <td><%="#{test.start_time.day}日"%></td>
     <td><%=test.nickname%></td>
        <td><%="#{test.intime}時〜#{test.outtime}時"%></td>

  </tr>  
  <%end%>

</table>




